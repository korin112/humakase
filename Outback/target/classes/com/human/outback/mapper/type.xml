<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.human.outback.iType">

<insert id="addType">
	insert into menutype(mtype_code, mtype_name) values(#{param1},#{param2})
</insert>
<select id="getMenutype" resultType="com.human.outback.Menutype">
   select mtype_code,mtype_name from menutype1 order by mtype_code
</select>
</mapper>